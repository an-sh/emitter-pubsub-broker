{
  "name": "emitter-pubsub-broker",
  "version": "1.0.0",
  "private": false,
  "description": "An utility for connecting EventEmitters via a pubsub.",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate-docs": "jsdoc --configure .jsdoc.json --verbose",
    "test": "nyc mocha"
  },
  "author": {
    "name": "Andrey Sharapov",
    "email": "an.sh.1@yandex.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/an-sh/emitter-pubsub-broker.git"
  },
  "keywords": [
    "EventEmitter",
    "emitter",
    "pubsub",
    "redis",
    "broker"
  ],
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.7.2",
    "eventemitter3": "^4.0.0",
    "ioredis": "^4.14.1",
    "msgpack-lite": "^0.1.20"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "codecov": "^3.6.1",
    "docdash": "^1.1.1",
    "event-to-promise": "^0.8.0",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "standard": "^14.3.1"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
